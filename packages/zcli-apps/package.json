    "fs-extra": "^10.0.0",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/adm-zip": "^0.5.0",
    "@types/archiver": "^5.3.1",
    "@types/chai": "^4",
    "@types/cors": "^2.8.6",
